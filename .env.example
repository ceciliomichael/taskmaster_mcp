# AI API Configuration (OpenAI-Compatible)
# Set these to use any OpenAI-compatible API endpoint

# Your API key for the AI service
AI_API_KEY=your_api_key_here

# API endpoints (defaults to OpenAI if not specified)
AI_EMBEDDINGS_URL=https://api.openai.com/v1/embeddings
AI_CHAT_URL=https://api.openai.com/v1/chat/completions

# Model names
AI_EMBEDDING_MODEL=text-embedding-3-small
AI_CHAT_MODEL=gpt-4

# Examples for different providers:
# 
# OpenAI (default):
# AI_API_KEY=sk-...
# AI_EMBEDDINGS_URL=https://api.openai.com/v1/embeddings
# AI_CHAT_URL=https://api.openai.com/v1/chat/completions
# AI_EMBEDDING_MODEL=text-embedding-3-small
# AI_CHAT_MODEL=gpt-4
#
# Anthropic (via OpenAI-compatible proxy):
# AI_API_KEY=your_anthropic_key
# AI_EMBEDDINGS_URL=https://your-proxy.com/v1/embeddings
# AI_CHAT_URL=https://your-proxy.com/v1/chat/completions
# AI_EMBEDDING_MODEL=text-embedding-3-small
# AI_CHAT_MODEL=claude-3-sonnet
#
# Local/Self-hosted (like Ollama with OpenAI compatibility):
# AI_API_KEY=not-needed-for-local
# AI_EMBEDDINGS_URL=http://localhost:11434/v1/embeddings
# AI_CHAT_URL=http://localhost:11434/v1/chat/completions
# AI_EMBEDDING_MODEL=nomic-embed-text
# AI_CHAT_MODEL=llama3.1
#
# Mistral (via OpenAI-compatible):
# AI_API_KEY=your_mistral_key
# AI_EMBEDDINGS_URL=https://api.mistral.ai/v1/embeddings
# AI_CHAT_URL=https://api.mistral.ai/v1/chat/completions
# AI_EMBEDDING_MODEL=mistral-embed
# AI_CHAT_MODEL=mistral-large-latest
